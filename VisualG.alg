c <- a
a <- b
b <- c

escreval ("*Agora o valor de a é: ",a," *")
escreval ("*Agora o valor de b é: ",b," *")
escreval ("--------------------------")
escreval ("")
escreval ("------------------------------------------------------------------")
escreval ("Esse é o poder de Ednaldo Pereira=932183901283901283948129041.....")
fimalgoritmo
