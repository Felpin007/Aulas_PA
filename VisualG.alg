// Seção de Declarações 
var
a,b,c: inteiro
inicio
// Seção de Comandos

a <- 10
b <- 20

escreval ("--------------------------")
escreval ("*O valor de a é: ",a," *")
escreval ("*O valor de b é: ",b," *")
